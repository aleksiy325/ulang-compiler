PROG fact
FUNC factorial (I)I
{
TEMP 0:I;
TEMP 1:I;
TEMP 2:I;
TEMP 3:Z;
TEMP 4:I;
TEMP 5:I;
TEMP 6:I;
TEMP 7:I;
TEMP 8:I;
T2 := 1;
T3 := T0 I== T2;
T3 := Z! T3;
IF T3 GOTO L0;
T4 := 1;
T1 := T4;
GOTO L1;
L0:;
T6 := 1;
T7 := T0 I- T6;
T5 := CALL factorial(T7);
T8 := T0 I* T5;
T1 := T8;
L1:;
RETURN T1;
}

FUNC main ()V
{
TEMP 0:U;
TEMP 1:I;
TEMP 2:I;
T0 := "The factorial of 8 is ";
PRINTU T0;
T2 := 8;
T1 := CALL factorial(T2);
PRINTLNI T1;
RETURN;
}

